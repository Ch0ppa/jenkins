pipeline {
    agent {
        label 'windows-shared'
    }
    options {
    	skipDefaultCheckout()
    }
    environment {
         PROJECT_SOLUTION_NAME = 'aspnet-get-started.sln'
    }
	stages {
	stage('Checkout') {
            steps {
                bat "git clone https://github.com/Ch0ppa/jenkins.git ${BUILD_NUMBER}"
                }}
	stage('checkDir') {
            steps {
                bat "dir ${WORKSPACE}\\${BUILD_NUMBER}" 
                }}	
		}
		}
